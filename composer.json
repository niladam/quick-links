{
    "name": "niladam/quick-links",
    "description": "Quickly open resource, and other files from within filament resource table in your PHPstorm editor.",
    "keywords": [
        "phpstorm",
        "editor",
        "quicklink",
        "shortcut",
        "laravel",
        "quick-links"
    ],
    "homepage": "https://github.com/niladam/quick-links",
    "support": {
        "issues": "https://github.com/niladam/quick-links/issues",
        "source": "https://github.com/niladam/quick-links"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Madalin Tache",
            "email": "niladam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Niladam\\QuickLinks\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niladam\\QuickLinks\\QuickLinksServiceProvider"
            ],
            "aliases": {
                "QuickLinks": "Niladam\\QuickLinks\\Facades\\QuickLinks"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
